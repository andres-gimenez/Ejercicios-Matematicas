{
    "files.autoSave": "afterDelay",
    "cSpell.language": "es-ES,en-UK",
    "cSpell.words": [
		"browser",
		"IES"
	],
    // "go.formatTool": "goimports",
	// "go.docsTool": "guru",
	// "editor.largeFileOptimizations": false,
